{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "Chatroom",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
      "source": "**",
      "destination": "/index.html"
    }
  ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
